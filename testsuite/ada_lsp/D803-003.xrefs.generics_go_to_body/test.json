[
   {
      "comment": [
         "test that textDocument/definition works well with generics"
      ]
   },
   {
      "start": {
         "cmd": [
            "${ALS}"
         ]
      }
   },
   {
      "send": {
         "request": {
            "params": {
               "processId": 2101,
               "capabilities": {
                  "workspace": {
                     "applyEdit": false
                  }
               },
               "rootUri": "$URI{.}"
            },
            "jsonrpc": "2.0",
            "id": 1,
            "method": "initialize"
         },
         "wait": [
            {
               "id": 1,
               "result": {
                  "capabilities": {
                     "typeDefinitionProvider": true,
                     "hoverProvider": true,
                     "definitionProvider": true,
                     "renameProvider": {},
                     "alsCalledByProvider": true,
                     "referencesProvider": true,
                     "textDocumentSync": 2,
                     "completionProvider": {
                        "triggerCharacters": [
                           "."
                        ],
                        "resolveProvider": false
                     },
                     "documentSymbolProvider": true
                  }
               }
            }
         ]
      }
   },
   {
      "send": {
         "request": {
            "jsonrpc": "2.0",
            "method": "initialized"
         },
         "wait": []
      }
   },
   {
      "send": {
         "request": {
            "params": {
               "settings": {
                  "ada": {
                     "projectFile": "default.gpr",
                     "scenarioVariables": {},
                     "enableDiagnostics": false,
                     "defaultCharset": "ISO-8859-1"
                  }
               }
            },
            "jsonrpc": "2.0",
            "method": "workspace/didChangeConfiguration"
         },
         "wait": []
      }
   },
   {
      "send": {
         "request": {
            "params": {
               "textDocument": {
                  "text": "with Gen;\nwith Genp;\n\nprocedure Main is\n   package G is new Gen;\n   procedure P is new Genp;\nbegin\n   G.Foo;\n   P;\nend Main;\n",
                  "version": 0,
                  "uri": "$URI{main.adb}",
                  "languageId": "Ada"
               }
            },
            "jsonrpc": "2.0",
            "method": "textDocument/didOpen"
         },
         "wait": []
      }
   },
   {
      "send": {
         "request": {
            "params": {
               "textDocument": {
                  "text": "package body Gen is\n   procedure Foo is\n   begin\n      null;\n   end Foo;\nend Gen;\n",
                  "version": 0,
                  "uri": "$URI{gen.adb}",
                  "languageId": "Ada"
               }
            },
            "jsonrpc": "2.0",
            "method": "textDocument/didOpen"
         },
         "wait": []
      }
   },
   {
      "send": {
         "request": {
            "params": {
               "textDocument": {
                  "text": "generic\npackage Gen is\n   procedure Foo;\nend Gen;\n",
                  "version": 0,
                  "uri": "$URI{gen.ads}",
                  "languageId": "Ada"
               }
            },
            "jsonrpc": "2.0",
            "method": "textDocument/didOpen"
         },
         "wait": [
         ]
      }
   },
   {
      "send": {
         "request": {
            "params": {
               "position": {
                  "line": 7,
                  "character": 5
               },
               "textDocument": {
                  "uri": "$URI{main.adb}"
               }
            },
            "jsonrpc": "2.0",
            "id": 2,
            "method": "textDocument/definition"
         },
         "wait": [
            {
               "id": 2,
               "result": [
                  {
                     "range": {
                        "start": {
                           "line": 2,
                           "character": 13
                        },
                        "end": {
                           "line": 2,
                           "character": 16
                        }
                     },
                     "uri": "$URI{gen.ads}"
                  }
               ]
            }
         ]
      }
   },
   {
      "send": {
         "request": {
            "params": {
               "position": {
                  "line": 2,
                  "character": 13
               },
               "textDocument": {
                  "uri": "$URI{gen.ads}"
               }
            },
            "jsonrpc": "2.0",
            "id": 3,
            "method": "textDocument/definition"
         },
         "wait": [
            {
               "id": 3,
               "result": [
                  {
                     "range": {
                        "start": {
                           "line": 1,
                           "character": 13
                        },
                        "end": {
                           "line": 1,
                           "character": 16
                        }
                     },
                     "uri": "$URI{gen.adb}"
                  }
               ]
            }
         ]
      }
   },
   {
      "send": {
         "request": {
            "params": {
               "position": {
                  "line": 8,
                  "character": 3
               },
               "textDocument": {
                  "uri": "$URI{main.adb}"
               }
            },
            "jsonrpc": "2.0",
            "id": 4,
            "method": "textDocument/definition"
         },
         "wait": [
            {
               "id": 4,
               "result": [
                  {
                     "range": {
                        "start": {
                           "line": 5,
                           "character": 13
                        },
                        "end": {
                           "line": 5,
                           "character": 14
                        }
                     },
                     "uri": "$URI{main.adb}"
                  }
               ]
            }
         ]
      }
   },
   {
      "send": {
         "request": {
            "params": {
               "textDocument": {
                  "uri": "$URI{main.adb}"
               }
            },
            "jsonrpc": "2.0",
            "method": "textDocument/didClose"
         },
         "wait": []
      }
   },
   {
      "send": {
         "request": {
            "params": {
               "textDocument": {
                  "uri": "$URI{gen.ads}"
               }
            },
            "jsonrpc": "2.0",
            "method": "textDocument/didClose"
         },
         "wait": []
      }
   },
   {
      "send": {
         "request": {
            "params": {
               "textDocument": {
                  "uri": "$URI{gen.adb}"
               }
            },
            "jsonrpc": "2.0",
            "method": "textDocument/didClose"
         },
         "wait": []
      }
   },
   {
      "send": {
         "request": {
            "jsonrpc": "2.0",
            "id": 6,
            "method": "shutdown"
         },
         "wait": [
            {
               "id": 6,
               "result": null
            }
         ]
      }
   },
   {
      "stop": {
         "exit_code": 0
      }
   }
]
